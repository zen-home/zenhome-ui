{
  "name": "zenhome",
  "version": "0.1.3",
  "description": "the web and mobile app for zenhome",
  "productName": "Zen Home | A home for all your home needs",
  "author": "James harrington <jamesdonaldharrington@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "eslint": "eslint",
    "lint": "eslint --ext .js,.vue --fix ./src ./mock ./test/* ./*.js",
    "dev": "concurrently -p '[{name}]' -n 'MOCK,QUASAR' -c 'bgYellow.bold,bgBlue.bold' 'npm run dev:mock-gql' 'quasar dev'",
    "dev:mock-gql": "nodemon -w mock mock/graphql/server.js -q",
    "posttest": "nyc merge .nyc_output out.json && nyc report --reporter=text --reporter=html --reporter=lcov --input-file out.json",
    "test": "npm run test:unit:ci && npm run test:component:ci && npm run test:e2e:ci",
    "build": "quasar build",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest --coverage",
    "test:unit:ci": "vitest run --coverage",
    "test:component": "cross-env NODE_ENV=test TEST_TYPE=component cypress open --component --env ENV=test",
    "test:component:ci": "cross-env NODE_ENV=test TEST_TYPE=component cypress run --component --env ENV=test",
    "test:e2e": "cross-env DEBUG=start-server-and-test NODE_ENV=test TEST_TYPE=e2e concurrently --kill-others -p '[{name}]' -n 'CYPRESS,DEV' -c 'bgGreen.bold,bgBlue.bold' 'node scripts/e2e-test.js \"cypress open --e2e\"' 'npm run dev'",
    "test:e2e:ci": "cross-env DEBUG=start-server-and-test NODE_ENV=test TEST_TYPE=e2e concurrently --kill-others -p '[{name}]' -n 'CYPRESS,DEV' -c 'bgGreen.bold,bgBlue.bold' 'node scripts/e2e-test.js \"cypress run --e2e\"' 'npm run dev'",
    "lhci": "lhci autorun",
    "lt": "lt --port 2340 --local-https --allow-invalid-cert",
    "serve": "quasar serve dist/spa",
    "prepare": "husky install",
    "release": "chmod 775 ./scripts/* && ./scripts/release.sh",
    "start-sonar": "docker-compose up -d sonarqube",
    "static-code-analysis": "chmod 775 ./scripts/* &&  ./scripts/sonar-scanner.sh",
    "docs:gen": "jsdoc -c jsdoc.json",
    "docs:serve": "npx serve docs",
    "gpt": "huskygpt review -e .js,.vue",
    "testgpt": "chmod 775 ./scripts/* && node ./scripts/testgpt-prompt.js"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "axios": "^1.6.1",
    "ink-docstrap": "^1.3.2",
    "pinia": "^2.1.7",
    "quasar": "^2.14.2",
    "vue": "^3.3.8",
    "vue-i18n": "^9.6.5",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@apollo/server": "^4.9.5",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "@cypress/code-coverage": "^3.12.9",
    "@lhci/cli": "^0.12.0",
    "@quasar/app-vite": "^2.0.0-alpha.44",
    "@quasar/quasar-app-extension-testing": "^2.2.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.2.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.4.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/compiler-dom": "^3.3.8",
    "@vue/server-renderer": "^3.3.8",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "c8": "^8.0.1",
    "clean-jsdoc-theme": "^4.2.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.5.0",
    "eslint": "^8.53.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vitest": "^0.3.9",
    "eslint-plugin-vue": "^9.18.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-compose": "^9.0.10",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^9.0.0",
    "happy-dom": "^12.10.3",
    "http-proxy": "^1.18.1",
    "husky": "^8.0.3",
    "inquirer": "^9.2.12",
    "jest-sonar-reporter": "^2.0.0",
    "js-yaml": "^4.1.0",
    "jsdoc": "^4.0.2",
    "jsdoc-vuejs": "^4.0.0",
    "localtunnel": "^1.8.3",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "ora": "^7.0.1",
    "playwright-webkit": "^1.39.0",
    "postcss": "^8.4.31",
    "sass": "^1.32.12",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.3",
    "vite-jsconfig-paths": "^2.0.1",
    "vitest": "^0.34.6",
    "webpack": "^5.89.0",
    "yargs": "^17.7.2"
  },
  "engines": {
    "node": "^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ]
  }
}
