{
  "name": "zenhome",
  "version": "0.1.3",
  "description": "the web and mobile app for zenhome",
  "productName": "Zen Home | A home for all your home needs",
  "author": "James harrington <jamesdonaldharrington@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --fix --ext .js,.vue .",
    "test": "npm run test:unit:ci && npm run test:component:ci && npm run test:e2e:ci",
    "dev": "concurrently -p \"[{name}]\" -n \"MOCK,QUASAR\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev:mock-gql\" \"quasar dev\"",
    "dev:mock-gql": "nodemon -w mock mock/graphql/server.js -q",
    "build": "quasar build",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:component": "cross-env NODE_ENV=test cypress open --component --env ENV=test",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component --env ENV=test",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:2340 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:2340 \"cypress run --e2e\"",
    "lhci": "lhci autorun",
    "lt": "lt --port 2340 --local-https --allow-invalid-cert",
    "serve": "quasar serve dist/spa",
    "prepare": "husky install",
    "release": "chmod 775 ./scripts/* && ./scripts/release.sh",
    "start-sonar": "docker-compose up -d sonarqube",
    "static-code-analysis": "chmod 775 ./scripts/* &&  ./scripts/sonar-scanner.sh",
    "docs:gen": "jsdoc -c jsdoc.json",
    "docs:serve": "npx serve docs",
    "gpt": "huskygpt review -e .js,.vue",
    "testgpt": "chmod 775 ./scripts/* && node ./scripts/testgpt-prompt.js",
    "check-licenses": "license-checker --exclude 'MIT, MIT OR X11, BSD, ISC' --production --json --out licenses.json",
    "snyk-test": "snyk test"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@cypress/code-coverage": "^3.12.15",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@lhci/cli": "^0.13.0",
    "@quasar/app-vite": "^1.3.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.2.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.4.0",
    "@vitest/ui": "^0.34.0",
    "@vue/compiler-dom": "^3.3.13",
    "@vue/server-renderer": "^3.3.13",
    "@vue/test-utils": "^2.4.1",
    "apollo-server-express": "^3.13.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "c8": "^8.0.1",
    "clean-jsdoc-theme": "^4.2.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.2",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.20.0",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-compose": "^9.0.10",
    "graphql-tools": "^9.0.0",
    "happy-dom": "^13.0.0",
    "http-proxy": "^1.18.1",
    "husky": "^8.0.3",
    "inquirer": "^9.2.12",
    "jest-sonar-reporter": "^2.0.0",
    "js-yaml": "^4.1.0",
    "jsdoc": "^4.0.2",
    "jsdoc-vuejs": "^4.0.0",
    "license-checker": "^25.0.1",
    "localtunnel": "^2.0.2",
    "nodemon": "^3.0.2",
    "ora": "^8.0.1",
    "playwright-webkit": "^1.40.1",
    "postcss": "^8.4.33",
    "sass": "^1.69.7",
    "snyk": "^1.1269.0",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.3",
    "vite-jsconfig-paths": "^2.0.1",
    "vitest": "^0.34.6",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "yargs": "^17.7.2"
  },
  "resolutions": {
    "@vitejs/plugin-vue": "^4.0.0",
    "vite": "^4.0.0"
  },
  "engines": {
    "node": "^20",
    "npm": "^10"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ]
  }
}
