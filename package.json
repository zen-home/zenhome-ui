{
  "name": "zenhome",
  "version": "0.0.1",
  "description": "the web and mobile app for zenhome",
  "productName": "Zen Home | A home for all your home needs",
  "author": "James harrington <jamesdonaldharrington@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:2340 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:2340 \"cypress run --e2e --browser webkit\"",
    "test:webkit": "cross-env NODE_ENV=test DEBUG=cypress:launcher start-test \"quasar dev\" http-get://localhost:2340 \"cypress run --browser webkit\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "pinia": "^2.0.11",
    "quasar": "^2.12.2",
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.8",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.3.0",
    "@quasar/quasar-app-extension-testing": "^2.1.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.1.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.3.0",
    "@quasar/vite-plugin": "^1.4.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-istanbul": "^0.32.2",
    "@vitest/coverage-v8": "^0.32.2",
    "@vitest/ui": "^0.15.0",
    "@vue/compiler-dom": "^3.3.4",
    "@vue/server-renderer": "^3.3.4",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "c8": "^8.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.2.0",
    "eslint": "^8.10.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "happy-dom": "^9.20.3",
    "playwright-webkit": "^1.35.1",
    "postcss": "^8.4.14",
    "sass": "1.32.12",
    "start-server-and-test": "^2.0.0",
    "vite": "^4.3.9",
    "vite-jsconfig-paths": "^2.0.1",
    "vite-plugin-istanbul": "^4.1.0",
    "vitest": "^0.15.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
