{
  "name": "zenhome",
  "version": "0.1.3",
  "description": "the web and mobile app for zenhome",
  "productName": "Zen Home | A home for all your home needs",
  "author": "James harrington <jamesdonaldharrington@gmail.com>",
  "private": true,
  "scripts": {
    "eslint": "eslint",
    "lint": "eslint --ext .js,.vue ./src ./mock ./test/* ./*.js",
    "test": "npm run test:unit:ci && npm run test:component:ci && npm run test:e2e:ci",
    "dev": "concurrently -p \"[{name}]\" -n \"MOCK,QUASAR\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev:mock-gql\" \"quasar dev\"",
    "dev:mock-gql": "nodemon -w mock mock/graphql/server.js -q",
    "build": "quasar build",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:component": "cross-env NODE_ENV=test cypress open --component --env ENV=test",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component --env ENV=test",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:2340 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:2340 \"cypress run --e2e\"",
    "lhci": "lhci autorun",
    "lt": "lt --port 2340 --local-https --allow-invalid-cert",
    "serve": "quasar serve dist/spa",
    "prepare": "husky install",
    "release": "chmod 775 ./scripts/* && ./scripts/release.sh",
    "start-sonar": "docker-compose up -d sonarqube",
    "static-code-analysis": "chmod 775 ./scripts/* &&  ./scripts/sonar-scanner.sh",
    "docs:gen": "jsdoc -c jsdoc.json",
    "docs:serve": "npx serve docs",
    "gpt": "huskygpt review -e .js,.vue",
    "testgpt": "chmod 775 ./scripts/* && node ./scripts/testgpt-prompt.js"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "ink-docstrap": "^1.3.2",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@cypress/code-coverage": "^3.10.8",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@lhci/cli": "^0.12.0",
    "@quasar/app-vite": "^1.3.0",
    "@quasar/quasar-app-extension-testing": "^2.1.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.1.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.1.0",
    "@vitest/ui": "^0.15.0",
    "@vue/compiler-dom": "^3.3.4",
    "@vue/server-renderer": "^3.3.4",
    "@vue/test-utils": "^2.0.0",
    "@zen-home/quasar-app-extension-testing": "^1.0.0-beta.15",
    "apollo-server-express": "^3.12.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.0.2",
    "c8": "^8.0.0",
    "clean-jsdoc-theme": "^4.2.9",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.2.0",
    "eslint": "^8.10.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "express": "^4.18.2",
    "graphql": "^16.7.1",
    "graphql-compose": "^9.0.10",
    "graphql-tools": "^9.0.0",
    "happy-dom": "^9.20.3",
    "http-proxy": "^1.18.1",
    "husky": "^8.0.0",
    "inquirer": "^9.2.11",
    "jest-sonar-reporter": "^2.0.0",
    "js-yaml": "^4.1.0",
    "jsdoc": "^4.0.2",
    "jsdoc-vuejs": "^4.0.0",
    "localtunnel": "^2.0.2",
    "nodemon": "^3.0.0",
    "ora": "^7.0.1",
    "playwright-webkit": "^1.35.1",
    "postcss": "^8.4.14",
    "sass": "1.32.12",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.3",
    "vite-jsconfig-paths": "^2.0.1",
    "vitest": "^0.15.0",
    "wait-on": "^7.0.1",
    "webpack": "^4.0.0",
    "yargs": "^17.7.2"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ]
  }
}
